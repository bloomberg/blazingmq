{
    "problemMatcher": [
        {
            "owner": "cpp-prometheus",
            "pattern": [
                {
                    "regexp":
                        "^(.*/src/plugins/bmqprometheus/.+):(\\d+):(\\d+): (.+): (.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            ]
        }
    ]
}
